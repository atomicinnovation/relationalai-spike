#!/usr/bin/env bash

PROJECT_DIR="$(pwd)"
PATH_add "${PROJECT_DIR}"

if has asdf; then
  asdf install
fi

python -m venv .venv
source .venv/bin/activate

if ! has rai; then
  pip install relationalai
fi

if ! has snow; then
  pip install snowflake-cli-labs
fi

if ! has jupyter; then
  pip install jupyter
fi
